public class RedeemAction extends Action<String> {
    String data;
    public static final String ACTION_REQUEST_START = "redeem_back_action_request_start";
    public static final String ACTION_REQUEST_FINISH = "redeem_back_action_request_finish";
    public static final String ACTION_REQUEST_FAIL = "redeem_back_action_request_fail";
    public static final String ACTION_REQUEST_ERROR_MESSAGE = "redeem_back_action_request_error_message";
    public static final String ACTION_REQUEST_INVALID_TOKEN = "redeem_back_action_request_invalid_token";
    public static final String ACTION_REQUEST_REDEEM_BACK_SUCCESS = "redeem_back_action_request_success";
    public RedeemAction(String type, String data) {
        super(type, data);
        this.data = data;
    }

    public RedeemAction(String type) {
        super(type);
    }
}
